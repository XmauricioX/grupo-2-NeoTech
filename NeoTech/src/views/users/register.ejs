                            <!-- email -->
                            <label for="email"></label>
                            <input class="email" type="email" name="email" id="email" placeholder="Ingrese su email">
                            <!-- password -->
                            <label for="password"></label>
                            <input class="password" type="password" id="password" placeholder="Ingrese su contraseÃ±a">
                            <!-- button log in -->
                            <button class="button-login" type="submit" value="log-in">Registrarse</button>
                        </nav>
                    </form>
                </div>

                <div class="networks">
                    <!-- button facebook -->
                    <button class="button-facebook"><img class="logo-face" src="/images/icons/icon-login-face.png"
